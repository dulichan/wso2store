body .store-left {
    padding-top: 0;
}
.asset-description {
    position: relative;
    padding-top: 0;
}
.asset-description-header h2 {
    margin-top: 12px;
}
.asset-description-header {
    background: #fff;
}
.asset-description-header .span9 {
    position: relative;
    height: 300px;
  
}
.asset-description-header .row{
    margin-bottom:0;
}

#modal-add-gadget code {
    display: block;
}

#modal-add-gadget .copy-status {
    margin-right: 20px;
}
.assetp-properties{
    margin-top:10px;
}
/*
.assetp-properties li:nth-child(even) {
    background: #f5f5f5;
}
*/
.assetp-properties .tab-content {
    overflow: visible;
}

#assetp-tabs > li > a {
    font-weight: 600;
        border-radius: 0 0 0 0;
        font-size:18px;
        color:#009E92;
        border-top:4px solid #FFFFFF;
padding:20px 25px;
}
#assetp-tabs > li > a:hover{
    background:none;
  color:#07BFAC;
  border-color:#FFF;

}
#assetp-tabs > li.active > a:hover{
    color:#07BFAC;
}

.nav-tabs{
    border-bottom:0;
}
#assetp-tabs > li > a:first-child{
    margin-left:-15px;
    margin-right:15px;
}
#assetp-tabs > li.active > a {
    border-color: #009E92 -moz-use-text-color -moz-use-text-color;
    border-image: none;
    border-style: solid none none;
    border-width: 4px 0 0;

}

#tab-reviews li {
    background: none repeat scroll 0 0 transparent;
    border-bottom: 1px solid #CCCCCC;
    font-size: 14px;
    padding: 9px 0 5px;
}
#tab-reviews .content{
    position:relative;
}

#tab-reviews li p {
    padding-bottom: 0;
    margin-bottom: 0;
}
#tab-reviews ul {
    position: relative;
}

#tab-reviews .tab-reviewer {
    color: #888888;
    font-size: 12px;
    font-style: italic;
    text-align: right;
}
#tab-review-box {
    padding: 10px;
    background: #F5F5F5;
}
#tab-review-box .row-fluid {
    margin-bottom: 0;
}

#reviews-loader {
    display: none;
}

.asset-banner {
     left:-15px;   
}

.asset-intro-box {
    left:564px;
}  

a.btn-embed {
    border-radius: 4px 4px 4px 4px;
    color: #AAAAAA;
    font-size: 18px;
    letter-spacing: 3px;
    padding: 10px;
    background:#ECF0F1;
   margin-top:0;
   display:inline-block;
}

a.btn-embed:hover{
    background:#ECF0F1;
    color:#2C3238;
}

@media all and (max-width: 1225px) {
.asset-intro-box {
    left: 450px;
}
}