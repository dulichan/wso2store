    border-bottom: 1px solid #ddd !important;
    padding: 15px 0;
    color: #7f8c8d;
}
.row-footer li {
    display: inline-block;
    margin-left: 20px;
}
.row-footer li a {
    color: #7f8c8d;
}
.row-footer li a:hover {
    color: #16a085;
}
