}

.asset-category {

}

.asset-add-btn {
    bottom: 20px;
    font-weight: 600;
    left: 15px;
    padding: 10px;
    position: absolute;
    right: 15px;
    text-align: center;
    color: #FFFFFF !important;
}
.asset-btn-cont {
    border-top: 1px solid #CCCCCC;
    margin-top: 10px;
