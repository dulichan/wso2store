}

.loading {
    position: absolute;
    background: rgba(255,255,255,.6);
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 100;
}

.loading img {
    left: 50%;
    position: absolute;
    top: 20%;
    opacity: .7;
}

.asset-banner {
    height: 299px;
    width: 580px;
    position: absolute;
    left: 0;
    border-bottom: 1px solid #ECF0F1;
    overflow: hidden;
}
.asset-intro-box {
    position: absolute;
    right: -20px;
    bottom: 0;
    top: 0;
    left: 580px;
    padding: 20px;
    border-left: 1px solid #ECF0F1;
    border-bottom: 1px solid #ECF0F1;
}
div.rating-cancel, div.star-rating {
    background-color: #fff !important;
    margin-top: 2px;
}
.asset-intro-box img {
    width: 100px;
    border: 1px solid #FFFFFF;
}
.asset-intro-box h3 {
    margin-bottom: 5px;
    line-height: normal;
}
.asset-intro-box small {
    display: block;
    line-height: 1;
    color: #7f8c8d;
}
.asset-intro-box .asset-rating {
    margin-top: 10px;
    width: auto;
    margin-bottom: 15px;
}

#btn-add-gadget {
    margin-top: 0;
    display: inline-block;
    font-size: 18px;
    padding: 10px;
    font-weight: bold;
}

.assets-container {
    margin-top: 20px;
}

#asset-slideshow-next, #asset-slideshow-prev {
    position: absolute;
    top: 0;
    z-index: 999;
    font-size: 0;
    width: 40px;
    height: 299px;
    background: url('../img/asset-slideshow-controls.png') no-repeat center left;
    opacity: .8;
    transition: opacity .2s;
}
#asset-slideshow-next:hover, #asset-slideshow-prev:hover {
    opacity: 1;
}
#asset-slideshow-next {
    left: 0;
}
#asset-slideshow-prev {
    right: 0;
    background-position: center right;
}

#asset-slideshow-cont {
    height: 276px;
    display: block;
    overflow: hidden;
}
#asset-slideshow .slide {
    position: relative;
    height: 300px;
}
#asset-slideshow-cont .html_carousel {
    position: absolute;
    left: -15px;
    right: -15px;
    top: 0;
}


#container-assets{
    margin-top: 35px;
    
}

@media all and (max-width: 1225px) {
    .store-right {
        margin-left: 0;
    }
    .asset-details {
        /*top: 166px !important;*/
       border-left: none;
    }
    .asset {
        height: 246px;
        border-left: 1px solid #CCCCCC;
    }
    .asset-intro-box {
        background: none repeat scroll 0 0 #FFFFFF;
        border-bottom: 1px solid #ECF0F1;
        border-left: 1px solid #ECF0F1;
        bottom: 0;
        left: 464px;
        padding: 20px;
        position: absolute;
        right: 0;
        top: 0;
        width: 205px;
        z-index: 998;
    }
    .asset-intro-box h3{
        font-size:95%;
    }
}