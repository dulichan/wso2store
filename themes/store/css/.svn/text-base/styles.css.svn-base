@media screen {
    @font-face {
        font-family: 'Lato';
        font-style: normal;
        font-weight: normal;
        src: local('Lato'), local('Lato'), url('../font/Lato-Regular.ttf') format('truetype');
    }

    @font-face {
        font-family: 'Lato';
        font-style: normal;
        font-weight: bold;
        src: local('Lato'), local('Lato'), url('../font/Lato-Bold.ttf') format('truetype');
    }
}

body {
    font: 16px/1.531 'Lato', sans-serif;
    color: #34495E;
    height: 100%;
    padding-left: 25px;
    padding-top: 46px;
    background:url(../img/search-bg.png) repeat-x left -7px #ecf0f1;
}

ul {
    margin: 0;
}
ul li {
    list-style: none;
}
iframe {
    border: none;
}

a, a:visited, a:active {
    color: #16a085;
}
a:focus {
    outline: none;
}
a:hover {
    text-decoration: none;
}

h1, h2, h3, h4 {
    margin-bottom: 20px;
}
.error {
    background: none repeat scroll 0 0 #FFF0C4;
    border-top: 1px solid #F2C976;
    color: #34495E;
    font-size: 13px;
    padding: 0 6px;
    margin-top: 0;
}
