    position: absolute;
}

#feature-3-html5 {
    right: 170px;
    position: absolute;
    top: 20px;
}

#feature-4-responsive {
    left: 0;
    position: absolute;
    top: 20px;
}
.row-border-bottom {
    border-bottom: 1px solid #f3f3f3;
    padding-bottom: 3px;
}
.store-gadget {
    position: relative;
}

.store-gadget p {
    background: url("../img/store-gadget-gradient.png") no-repeat scroll left bottom transparent;
    bottom: -10px;
    color: #FFFFFF;
    font-weight: 600;
    left: 0;
    line-height: 18px;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 20px;
    position: absolute;
    right: 0;
    text-shadow: 0 1px 0 #000000;
}
.store-gadget p small {
    display: block;
    clear: both;
    color: #f5f5f5;
    color: #f5f5f5;
}
.store-left {
    position: relative;
    margin-bottom: 20px;
}

.store-left .row {
    margin-bottom: 20px;
}
.store-left h2 {
    margin-top: 0;
}
h3.asset-title-separator {
    color: #fff;
    padding-left: 72px;
    margin-top: 0;
    position: relative;
    font-size: 18px;
}

.store-left h3.asset-type-gadget {
    background: url("../img/icon-bar-gadget.png") no-repeat scroll 14px 9px #16a085;
}

.store-left h3.asset-type-site {
    background: url("../img/icon-bar-site.png") no-repeat scroll 14px 9px #16a085;
}

#search {

   /* background: url('../img/icon-search.png') #ffffff no-repeat right;*/
    margin-bottom: 0;
    font-size: 18px;
}
select, textarea, input[type="text"], input[type="password"], input[type="datetime"], input[type="datetime-local"], input[type="date"], input[type="month"], input[type="time"], input[type="week"], input[type="number"], input[type="email"], input[type="url"], input[type="search"], input[type="tel"], input[type="color"], .uneditable-input {
    border-radius: 0;
}

.recently-added {
    margin-bottom: 10px;
}
.recently-added .span3 img {
    width: 93%;
}
.recently-added small {
    display: block;
    clear: both;
}
.recently-added .star-rating {
    display: block;
    clear: both;
}
.store-right {
    padding-top: 7px;
    margin-left: 21px;
    padding-left: 20px;
    border-left: 1px solid #ecf0f1;
    background: none repeat scroll 0 0 #F7F9F9;
}
.store-right a {
    color: #34495E;
}
