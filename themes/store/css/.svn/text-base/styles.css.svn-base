    margin-right: 8px;
}
.store-right hr {
    border-color: #EEEEEE;
    box-shadow: 0 1px 0 #FFFFFF;
    margin-bottom: 0;
    margin-top: 22px;
    width: 90%;
}

.store-tags ul li {
    display: inline-block;
    margin-right: 6px;
    height: 40px;
}
.store-tags ul li a {
    padding: 5px;
    border: 1px solid #cacaca;
    background: #fcfcfc;
    color: #626262;
}
.store-tags ul li a:hover {
    background: #16a085;
    color: #ffffff;
    text-decoration: none;
}
.store-icon {
    margin-top: 10px;
    font-size: 22px;
}

#gadgets {
    opacity: 0;
}

.asset {
    border-bottom: 1px solid #CCCCCC;
    border-right: 1px solid #CCCCCC;
    height: 246px;
    overflow: hidden;
    position: relative;
}
.asset a:hover {
    text-decoration: none;
}
.asset-icon {
    position: relative;
}
.asset-icon-overlay {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,.7);
}
.asset-icon-overlay i {
    color: #FFFFFF;
    display: block;
    font-size: 44px;
    height: 100%;
    position: absolute;
    text-align: center;
    top: 45%;
    width: 100%;
}
.asset-icon > img {
    border: medium none;
    width: 100%;
    border-top: 1px solid #CCCCCC;
    border-left: 1px solid #CCCCCC;
    border-right: 1px solid #CCCCCC;
}

.asset-details {
    /*
     border-bottom: 1px solid #CCCCCC;
     border-left: 1px solid #CCCCCC;
     border-right: 1px solid #CCCCCC;
     padding: 5px 5px 0;
     */
    border-bottom: 1px solid #CCCCCC;
    border-left: 1px solid #CCCCCC;
    height: 246px;
    left: 0;
    padding: 5px 15px 0;
    position: absolute;
    right: 0;
    background: none repeat scroll 0 0 #FFFFFF;
    top: 200px;
}

.asset-rating {
    background: url('../img/store-stars.png') no-repeat top left;
    display: block;
    height: 20px;
    width: 84px;
    overflow: hidden;
}
.asset-rating > div {
    background: url('../img/store-stars.png') no-repeat bottom left;
    height: inherit;
}

.asset-rating-0star {
    width: 0;
    display: block;
}
.asset-rating-1star {
    width: 16px;
    display: block;
}
.asset-rating-2star {
    width: 32px;
    display: block;
}
.asset-rating-3star {
    width: 48px;
    display: block;
}
.asset-rating-4star {
    width: 64px;
    display: block;
}
.asset-rating-5star {
    width: 80px;
    display: block;
}

.asset-name {
    color: #34495E;
    font-size: 18px;
    font-weight: 600;
    max-height: 39px;
    overflow: hidden;
}
.asset-name a:hover i {
    color: #16a085;
}
.asset-name a i {

    position: absolute;
    font-size: 18px;
    top: 10px;
    right: 11px;
    display: none;
    color: #AAAAAA;
}

.asset-name a h4 {
    color: #34495E;
    font-weight: bold;
    height: 20px;
    line-height: 18px;
    margin: 0;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 100%;
    font-size: 85%;
}
