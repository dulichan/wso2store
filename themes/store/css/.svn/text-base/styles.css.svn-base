.hero-unit {
    text-align: center;
    background: none;
}
.hero-unit p {
    font-style: italic;
    margin-top: 20px;
}
.separator {
    text-align: center;
    background-image: url("../img/separator-line.png");
    background-repeat: repeat-x;
    margin-bottom: 40px;
}
.separator span {
    background: none repeat scroll 0 0 #FFFFFF;
    border: 3px solid #E5E5E5;
    border-radius: 8px 8px 8px 8px;
    -moz-border-radius: 8px 8px 8px 8px;
    -webkit-border-radius: 8px 8px 8px 8px;
    font-size: 18px;
    font-weight: 600;
    padding: 10px 15px;
}
.txt-right {
    text-align: right;
}
.txt-center {
    text-align: center;
}
.btn {
    background-image: none;
    font-size: 16px;
}
a.btn-primary, button.btn-primary {
    background: none repeat scroll 0 0 #16a085;
    color: #FFFFFF;
    text-shadow: none;
}

a.btn-primary:hover, button.btn-primary:hover {
    background: #07BFAC;
}
.btn-footer {
    font-weight: 600;
    padding: 5px;
}
.btn-footer  i {
    font-size: 22px;
    line-height: 22px;
    vertical-align: middle;
}

.btn-footer:hover {
    text-decoration: none;
    opacity: .8;
}
#btn-github {
    background: #31B2FD;
    margin-right: 20px;
    color: #ffffff;
}
#btn-follow {
    border: 1px solid #ccc;
    color: #979797;
    background: #ffffff;
}
#btn-follow a {
    padding: 0 5px;
}

.border-radius-4 {
    border-radius: 4px;
    -moz-border-radius: 4px;
    -webkit-border-radius: 4px;
}
footer {
    border-top: 1px solid #16a085;
    padding-top: 30px;
    width: 100%;
    margin-left: auto;
    margin-right: auto;
}
footer.container {
    width: 100%;
}
footer.row {
    margin: 0 auto;
}
.alert-info {
    position: relative;
}

.alert-info span {
    display: block;
    margin: 0 auto;
    text-align: center;
    width: 300px;
}
.alert-info .close {
    position: absolute;
    right: 15px;
    top: 10px;
}

.layout {
    padding: 50px;
    font-family: Georgia, serif;
}
.layout-slider {
    margin-bottom: 60px;
    width: 80%;
    margin-left: auto;
    margin-right: auto;
}
.layout-slider-settings {
    font-size: 12px;
    padding-bottom: 10px;
}
.layout-slider-settings pre {
    font-family: Courier;
}

#timeslider {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: #CCCCCC;
    border-image: none;
    border-style: solid;
    border-width: 1px 1px 3px;
    padding-top: 30px;
    background: #ffffff;
}

.jslider .jslider-value, .jslider .jslider-label {
    font-size: 12px;
}

#sticky {
    background: #ffffff;
    -moz-box-shadow: 0 25px 20px #FFFFFF;
    -webkit-box-shadow: 0 25px 20px #FFFFFF;
    box-shadow: 0 25px 20px #FFFFFF;
    height: 110px;
    border-top: 15px solid #ffffff;
}

#sticky.stick {
    position: fixed;
    top: 65px;
    z-index: 10000;
}

#gadgets h2 {
    font-size: 18px;
}
h1:hover, h2:hover, h3:hover {
    color: #34495E;
}

.container > .row {
    background: #FFFFFF;
    border: 1px solid #ddd;
    border-bottom: 0;
}
.container:not(#container-search) > .row > div:first-child {
    margin-left: 15px;
}
